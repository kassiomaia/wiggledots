{
  "compilerOptions": {
    "target": "ES2024",
    "lib": [
      "DOM",
      "ES2024"
    ],
    "jsx": "react-jsx",
    "module": "commonjs",
    "paths": {
      "@src/containers/*": [
        "./src/containers/*"
      ],
      "@src/providers/*": [
        "./src/providers/*"
      ],
      "@src/images/*": [
        "./src/images/*"
      ],
      "@src/styles/*": [
        "./src/styles/*"
      ]
    },
    "noEmit": true,
    /* Interop Constraints */
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
